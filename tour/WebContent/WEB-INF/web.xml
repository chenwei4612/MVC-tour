  <servlet>
    <servlet-name>UnameCheckServlet</servlet-name>
    <servlet-class>servlet.UnameCheckServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UnameCheckServlet</servlet-name>
    <url-pattern>/user/page/userUnameCheckServlet</url-pattern>
