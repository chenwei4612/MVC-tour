<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <!-- 登陆 -->
  <servlet>
    <servlet-name>userLoginServlet</servlet-name>
    <servlet-class>servlet.UserLoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>userLoginServlet</servlet-name>
    <url-pattern>/user/page/userLoginServlet</url-pattern>
  </servlet-mapping>
  <!-- 注册 -->
  <servlet>
    <servlet-name>userRegistServlet</servlet-name>
    <servlet-class>servlet.UserRegistServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>userRegistServlet</servlet-name>
    <url-pattern>/user/page/userRegistServlet</url-pattern>
  </servlet-mapping>
  <!-- 旅游-->
  <servlet>
    <servlet-name>allTourServlet</servlet-name>
    <servlet-class>servlet.AllTourServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>allTourServlet</servlet-name>
    <url-pattern>/user/page/AllTourServlet</url-pattern>
  </servlet-mapping>
  <!-- 详情 -->
  <servlet>
    <servlet-name>detailServlet</servlet-name>
    <servlet-class>servlet.DetailTourServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>detailServlet</servlet-name>
    <url-pattern>/user/page/DetailTourServlet</url-pattern>
  </servlet-mapping>
